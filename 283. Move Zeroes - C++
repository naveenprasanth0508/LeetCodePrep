class Solution {
public:
    void moveZeroes(vector<int>& nums) {
        int LastNonZero = 0;
        for(int i = 0; i < nums.size(); i++) {
            if(nums[i] != 0) {
                if(i != LastNonZero)
                swap(nums[i], nums[LastNonZero]);
                LastNonZero++;
            }
        }
    }
};
