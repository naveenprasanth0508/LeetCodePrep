class Solution {
public:
    vector<int> majorityElement(vector<int>& nums) {
        unordered_map<int,int> mpp;
        for (int num : nums) mpp[num]++;
        
        vector<int> ans;
        for (auto &[key, value] : mpp)
            if (value > nums.size() / 3)
                ans.push_back(key);
        
        return ans;
    }
};
